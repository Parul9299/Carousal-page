<%- include('head') %>

  <body>

    <!-- ==============================NAVBAR============================ -->

    <%- include('navbar') %>

      <!-- ==============================CAROUSAL============================ -->

      <div class="container-fluid">
        <div class="row">
          <div class="col-md-12">
            <div id="news-slider" class="owl-carousel">
              <% slides.forEach((slide)=>{ %>
                <div class="post-slide">
                  <div class="post-img">
                    <img src="images/<%- slide.img %>.jpeg" alt="image not available">
                    <a href="#" class="over-layer"><i class="fa fa-link"></i></a>
                  </div>
                  <div class="post-content">
                    <h3 class="post-title">
                      <a href="#"><%- slide.title %></a>
                    </h3>
                    <p class="post-description"><%- slide.desc %></p>
                    <span class="post-date"><i class="fa fa-clock-o"></i>Out 27, 2019</span>
                    <a href="/ReadMore" class="read-more">read more</a>
                  </div>
                </div>
                <% }) %>
            </div>
          </div>
        </div>
      </div>



      <!-- ==========================UNIVERSITY-LOGOS============================== -->

      <div class="container mt-5">
        <div class="row">

          <div class="col-lg-12">
            <div class="heading">
              <h2>Approved Online & Distance Education Universities</h2>
            </div>
          </div>

          <% universities.forEach((university, index)=>{ %>
            <div class="col-lg-2 mb-3">
              <div class="partners_card">
                <a target="_blank" class="unibtn" data-bs-toggle="modal" data-bs-target="#uniModal<%- index %>"
                  href="#">
                  <div class="p-3 card partner_item">
                    <img title="university" width="186" height="auto" loading="lazy" class="img-fluid"
                      src="images/<%- university.img %>.png" alt="universityimage">
                    <div class="partner_content">
                      <h6><%- university.course %></h6>
                      <span><%- university.name %></span>
                    </div>
                  </div>
                </a>
              </div>
            </div>
          <% }) %>

        </div>

        <!-- ============================================MODAL=========================================== -->

        

              </div>
              <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary">Save changes</button>
              </div>
            </div>
          </div>
        </div>
      </div>




  </body>

  </html>
  <%- include('script') %>